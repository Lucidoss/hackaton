<html>
	<head>
		<meta charset="utf-8">
		<title>Inscription</title>
		<link rel="stylesheet" type="text/css" href="{{asset('styleconnexion.css')}}">
		<link rel="icon" type="image/x-icon" href="{{asset('images/logo.png')}}">
	</head>

	<body>
		<div class="login-root">
			{{ include('template.html.twig') }}
			<div class="box-root padding-top--24 flex-flex flex-direction--column" style="flex-grow: 1; z-index: 9;">
				<div class="box-root padding-top--48 padding-bottom--24 flex-flex flex-justifyContent--center">
					<h1>
						<a href="/" rel="dofollow">Profil</a>
					</h1>
				</div>
				<div class="formbg-outer">
					<div class="formbg">
						<div class="formbg-inner padding-horizontal--48">
							<span class="padding-bottom--15">Mes informations
							</span>
							<form id="stripe-login" action="{{ path('app_inscription', {uc: 'POST' }) }}" method="POST">
                                <div class="field padding-bottom--24">
									<label for="nom">Nom</label>
									<input type="text" name="nom" id="nom" minlength="1" maxlength="32" required value='{{ leProfil.Nom }}' disabled style="display: block;
																												    width: 100%;
																												    padding: 0.375rem 0.75rem;
																												    font-size: 1rem;
																												    line-height: 1.5;
																												    color: #495057;
																												    background-color: #e9ecef;
																												    background-clip: padding-box;
																												    border: 1px solid #ced4da;
																												    border-radius: 0.25rem;
																												    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
																													background-color">
								</div>
								<div class="field padding-bottom--24">
									<label for="prenom">Prenom</label>
									<input type="text" name="prenom" minlength="1" maxlength="32" required value='{{ leProfil.Prenom }}'disabled style="display: block;
																												    width: 100%;
																												    padding: 0.375rem 0.75rem;
																												    font-size: 1rem;
																												    line-height: 1.5;
																												    color: #495057;
																												    background-color: #e9ecef;
																												    background-clip: padding-box;
																												    border: 1px solid #ced4da;
																												    border-radius: 0.25rem;
																												    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
																													background-color">
								</div>
								<div class="field padding-bottom--24">
									<label for="naissance">Date de Naissance</label>
									<input type="date" name="dateNaissance" required value='{{ leProfil.DateNaissance|date('Y-m-d') }}'disabled style="display: block;
																												    width: 100%;
																												    padding: 0.375rem 0.75rem;
																												    font-size: 1rem;
																												    line-height: 1.5;
																												    color: #495057;
																												    background-color: #e9ecef;
																												    background-clip: padding-box;
																												    border: 1px solid #ced4da;
																												    border-radius: 0.25rem;
																												    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
																													background-color">
								</div>
								<div class="field padding-bottom--24">
									<label for="ville">Ville</label>
									<input type="text" name="ville" minlength="1" maxlength="64" required value='{{ leProfil.Ville }}'disabled style="display: block;
																												    width: 100%;
																												    padding: 0.375rem 0.75rem;
																												    font-size: 1rem;
																												    line-height: 1.5;
																												    color: #495057;
																												    background-color: #e9ecef;
																												    background-clip: padding-box;
																												    border: 1px solid #ced4da;
																												    border-radius: 0.25rem;
																												    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
																													background-color">
								</div>
								<div class="field padding-bottom--24">
									<label for="rue">Rue</label>
									<input type="text" name="rue" minlength="1" maxlength="128" required value='{{ leProfil.Rue }}'disabled style="display: block;
																												    width: 100%;
																												    padding: 0.375rem 0.75rem;
																												    font-size: 1rem;
																												    line-height: 1.5;
																												    color: #495057;
																												    background-color: #e9ecef;
																												    background-clip: padding-box;
																												    border: 1px solid #ced4da;
																												    border-radius: 0.25rem;
																												    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
																													background-color">
								</div>
								<div class="field padding-bottom--24">
									<label for="cp">Code Postal</label>
									<input type="text" name="cp" minlength="5" maxlength="5" required value='{{ leProfil.CP }}' disabled style="display: block;
																												    width: 100%;
																												    padding: 0.375rem 0.75rem;
																												    font-size: 1rem;
																												    line-height: 1.5;
																												    color: #495057;
																												    background-color: #e9ecef;
																												    background-clip: padding-box;
																												    border: 1px solid #ced4da;
																												    border-radius: 0.25rem;
																												    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
																													background-color">
								</div>
								<div class="field padding-bottom--24">
									<label for="email">Email</label>
									<input type="email" name="email" minlength="1" maxlength="32" required value='{{ leProfil.Email }}'disabled style="display: block;
																												    width: 100%;
																												    padding: 0.375rem 0.75rem;
																												    font-size: 1rem;
																												    line-height: 1.5;
																												    color: #495057;
																												    background-color: #e9ecef;
																												    background-clip: padding-box;
																												    border: 1px solid #ced4da;
																												    border-radius: 0.25rem;
																												    transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
																													background-color">
								</div>
							</form>
							{% if lesInscriptions == true %}
								<span class="padding-bottom--15">Mes Hacakthons
								</span>
								{% for hackathon in lesInscriptions %}
									<div class="field padding-bottom--24">
										<label for="hackathon">{{ hackathon.Hackathon.THEME }}</label>
										<a href="/detailhackathon/{{ hackathon.Hackathon.id }}"><input type="submit" name="submit" value="Voir le Hackathon"></a>
									</div>
								{% endfor %}
							{% endif %}
						</div>
						<div class="footer-link padding-top--24" style="margin-bottom:40px">
					</div>
				</div>
			</div>
		</div>
	</body>
</html></div></body></html>